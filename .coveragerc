# Coverage configuration

[run]
source = .
omit =
    tests/*
    **/__pycache__/*
    **/venv/*
    **/env/*
    **/.venv/*
    setup.py
    **/site-packages/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod

[html]
directory = htmlcov
